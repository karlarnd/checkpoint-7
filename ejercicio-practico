function calculo(numero1, numero2, numero3, numero4) {
    let total = (numero1 + numero2) * (numero3 + numero4);

    if (total > 50) {
        console.log("¡El número es mayor que 50!");
    }
    else if (total < 50) {
        console.log("¡El número es inferior a 50!");
    }
    else {
        console.log("¡El número es justo 50!");
    }
}
